<configuration>
    <property>
        <name>cli.paras</name>
        <value></value>
        <description>if not given parameters for cli, then use cli.paras</description>
    </property>
    <property>
        <name>page.validPeriod</name>
        <value>-1</value>
        <description>unit is second. For a month is 3600*24*30=2592000. If 0, is not save and not read from local cache;
            if negative, always read from local cache if exists
        </description>
    </property>
    <property>
        <name>download.concurrent.num</name>
        <value>1</value>
    </property>
    <property>
        <name>download.timeout</name>
        <value>20000</value>
        <description>socket timeout in milliseconds</description>
    </property>
    <property>
        <name>download.sleepMilliseconds</name>
        <value>20</value>
        <description>sleep milliseconds when between two web page download</description>
    </property>

    <property>
        <name>results.directory</name>
